cat daysOfTheYear2015 albumsByDate | tr '[\000-\011\013-\037\177-\377]' '.' | grep -v '^$' | sed "s/|/~/" | grep -v "July 31" | awk 'BEGIN{i="January 1";}{if($0 ~ i){print $0;}else{i=$1" "$2; print "\n---- "i" -----\n"$0}}' | cut -d'~' -f2- | awk 'BEGIN{i="-----";}{if ($0 ~ i){print $0"\nALBUMS:";}else{print $0}}' | sed 's/-----/-----------------------------\nTASKS:\n- Personal:\n- Work:/g' | sed 's/^ /- /g' | sed 's/^$/-----------------------------------------\n/g' | sed 's/ALBUMS/GYM:\n- Squats, Deadlifts, Bench\nALBUMS/g'
