cat listOfAlbums2015.txt | sed 's:|-:\n\n:g' | awk -F'<ref>' '{print $1}' | sed 's/<br\/>/ /g' | sed -e 's/^[ \t]*//' | sed '/^$/d'| sed 's/^! style="width:[0-9][0-9]%;"| //g' | sed s'/rowspan="[0-9]" style="text-align:center;"//g' | grep -A5000 -a '! Release date ! style="width:15%;" | Artist ! style="width:20%;" | Album ! style="width:20%;" | Genre ! style="width:15%;" | Label ! style="width:20%;" | Producer ! Ref.'| awk 'BEGIN{i=0;FS=" | ";}{if(/^\|/){print i,$0;}else{print $0,i=$1" "$2;}}'
